                                        <li bwb-ng-variabel-deel="/Hoofdstuk1/Paragraaf1.1/Artikel1/Onderdeelf/Onderdeel1">
                                            <li.nr>1°.</li.nr>
                                            <al>een instelling die zich blijkens haar doelstelling en feitelijke
                                                werkzaamheden richt op het bieden van verpleging of verzorging aan
                                                aldaar verblijvende hulpbehoevenden;
                                            </al>
                                            <meta-data>
                                                <jcis>
                                                    <jci versie="1.3"
                                                         verwijzing="jci1.3:c:BWBR0015703&amp;hoofdstuk=1&amp;paragraaf=1.1&amp;artikel=1&amp;o=f&amp;o=1&amp;z=2019-01-01&amp;g=2019-01-01"
                                                         onderdeel="o=1"/>
                                                </jcis>
                                            </meta-data>
                                        </li>
                                        <li bwb-ng-variabel-deel="/Hoofdstuk1/Paragraaf1.1/Artikel1/Onderdeelf/Onderdeel2">
                                            <li.nr>2°.</li.nr>
                                            <al>een instelling die zich blijkens haar doelstelling en feitelijke
                                                werkzaamheden richt op het bieden van slaapgelegenheid, waarbij de
                                                mogelijkheid van hulpverlening of begeleiding gedurende meer dan de
                                                helft van ieder etmaal aanwezig is;
                                            </al>
